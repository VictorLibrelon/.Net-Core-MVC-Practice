@{
    ViewData["Title"] = "Update Contact";
}

<div class="text-center">

    <h1 class="display-4">Update Contact</h1>

</div>
<form>

    <div class="form-group">
        <label>Name</label>
        <input type="text" class="form-control" placeholder="Enter Name">
    </div>
    <br />
    <div class="form-group">
        <label for="exampleInputEmail1">Email address</label>
        <input type="email" class="form-control" placeholder="Enter email">
    </div>
    <br />
    <div class="form-group">
        <label>Cell phone number</label>
        <input type="text" class="form-control" placeholder="Enter Cell phone number">
    </div>
    <br />

    <button type="submit" class="btn btn-primary ">Update</button>
    <a class="btn btn-secondary " asp-controller="Contact" asp-action="Index" role="button">Back</a>
</form>